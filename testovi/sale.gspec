@objects
 glavni .glavni
 meni .meni
 logo .logo
 sadrzaj .sadrzaj
 tabelaSala .tabelaSala
 lista .lista
 li .lista li
 list_item-* .lista li a
 slika #slika
 prvaCelija #prvaCelija
 headerRedni .headerRedni
 headerSprat .headerSprat
 zauzetaSala .zauzetaSala
 slobodnaSala .slobodnaSala
 sala .sala
 kancelarija .kancelarija

= Header section =
 glavni:
  width 30 to 40 % of screen/width
 meni:
  width 95 to 100 % of glavni/width
  inside screen 50px top
  above sadrzaj 10px
 li:
  css display is "inline"
 list_item-2:
  css text-decoration-line is "none"
  text contains "Sale"
 list_item-3:
  text contains "Unos"
 list_item-4:
  text contains "Rezervacije"
 list_item-5:
  text contains "Osobe"
 li:
  css list-style-type is "none"
 logo:
  width 70px
  height 70px
  contains slika
  inside partly meni -35px top
 comments:
  absent
= Main section =
 sadrzaj:
  width 100% of glavni/width
  css background-color is "rgb(100, 149, 237)"
 tabelaSala:
  inside sadrzaj 10 px bottom
 prvaCelija:
  absent
 headerRedni:
  css background-color is "rgb(211, 211, 211)"
  css text-align is "left"
 headerSprat:
  css background-color is "rgb(169, 169, 169)"
  css text-align is "center"
  height 40 to 60 px
 zauzetaSala:
  css background-color is "rgb(255, 69, 0)"
  css text-align is "left"
 slobodnaSala:
  css background-color is "rgb(173, 255, 47)"
  css text-align is "left"
 sala:
  css background-color is "rgb(255, 255, 255)"
  css text-align is "left"
 kancelarija:
  css background-color is "rgb(102, 51, 153)"
  css text-align is "left"
  